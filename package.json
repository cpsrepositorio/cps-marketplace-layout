{
  "name": "cps-marketplace-layout",
  "description": "Sample layout for the CPS Marketplace project built with Vue 3 + Vite + Tailwind.",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite-ssg build --base=/cps-marketplace-layout/",
    "preview": "vite preview --port 5050 --base=/cps-marketplace-layout/",
    "deploy": "sh ./deploy.sh",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:fix": "npm run lint --fix",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@vueuse/core": "^8.5.0",
    "pinia": "^2.0.13",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vueuse/head": "^0.7.6",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.5",
    "vite-ssg": "^0.20.0",
    "vite-svg-loader": "^3.3.0",
    "vue-router": "^4.0.15"
  }
}